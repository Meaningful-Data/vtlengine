IMMDT_PRNT_DS_OBS :=
    RIAD_IS_OWNR_C
        [calc measure IMMDT_PRNT_CD := ENTTY_RIAD_CD]
        [keep IMMDT_PRNT_CD];

IMMDT_PRNT_DS_MX :=
    max(IMMDT_PRNT_DS_OBS group except ENTTY_RIAD_CD);
    
