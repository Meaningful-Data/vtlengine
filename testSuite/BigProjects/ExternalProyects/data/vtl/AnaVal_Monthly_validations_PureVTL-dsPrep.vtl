LGL_FRM_DS_FLTR :=
    LGL_FRM_DS_CJ[filter DT_RFRNC >= VLD_FRM and DT_RFRNC <= VLD_T];
        
LGL_FRM_DS := 
    max(LGL_FRM_DS_FLTR group except VLD_FRM, VLD_T);



        
SSMSIGNIFICANCE_DS_MSR := 
    RIAD_SSMSIGNIFICANCE_C
