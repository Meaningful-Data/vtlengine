/***************************************************************************************

ENTTY_PD and ENTTY_INSTRMNT_CMA

****************************************************************************************/



ENTTY_INSTRMNT_1_DBTR :=
    inner_join(
        DBTRS,
        NMBR_DBTRS[filter int_var = 1] as B
    );


