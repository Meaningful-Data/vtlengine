    max(ULTMT_PRNT_DS_OBS group except ENTTY_RIAD_CD);
    
ULTMT_PRNT_DS_MSR :=
   ULTMT_PRNT_DS_MX
        [rename TRGT_ENTTY_RIAD_CD to ENTTY_RIAD_CD];

ULTMT_PRNT_DS_CJ :=
    cross_join(ULTMT_PRNT_DS_MSR, DT_RFRNC_DS);

ULTMT_PRNT_DS_FLTR :=
    ULTMT_PRNT_DS_CJ[filter DT_RFRNC >= VLD_FRM and DT_RFRNC <= VLD_T];
        
ULTMT_PRNT_DS := 
    max(ULTMT_PRNT_DS_FLTR group except VLD_FRM, VLD_T);
    
    
    
LEI_DS_MSR := 
    RIAD_TYP_ENTTY_CD_C
        [sub TYP_ENTTY_CD = "LEI"]
        [rename OBS_VALUE to LEI]
        [keep LEI];

LEI_DS_CJ :=
    cross_join(LEI_DS_MSR, DT_RFRNC_DS);

LEI_DS_FLTR :=
    LEI_DS_CJ[filter DT_RFRNC >= VLD_FRM and DT_RFRNC <= VLD_T];
        
LEI_DS := 
    max(LEI_DS_FLTR group except VLD_FRM, VLD_T);



NM_ENTTY_DS_MSR := 
    RIAD_NM_ENTTY_C
        [keep OBS_VALUE]
        [rename OBS_VALUE to NM_ENTTY];
        
NM_ENTTY_DS_CJ :=
    cross_join(NM_ENTTY_DS_MSR, DT_RFRNC_DS);

