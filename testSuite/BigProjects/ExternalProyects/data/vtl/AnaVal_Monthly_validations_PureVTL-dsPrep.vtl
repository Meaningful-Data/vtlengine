    INSTTTNL_SCTR_DS_CJ[filter DT_RFRNC >= VLD_FRM and DT_RFRNC <= VLD_T];
        
INSTTTNL_SCTR_DS := 
    max(INSTTTNL_SCTR_DS_CJ group except VLD_FRM, VLD_T);
        

        
