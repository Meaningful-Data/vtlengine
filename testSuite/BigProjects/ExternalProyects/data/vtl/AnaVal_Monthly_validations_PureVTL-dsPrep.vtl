    inner_join(
        ANCRDT_FNNCL_C,
        ENTTY_INSTRMNT_1_DBTR
        using DT_RFRNC, OBSRVD_AGNT_CD, CNTRCT_ID, INSTRMNT_ID
    );




ENTTY_DFLT_RSK := 
    left_join(
        ANCRDT_ENTTY_DFLT_C,
        ANCRDT_ENTTY_RSK_C
    );

