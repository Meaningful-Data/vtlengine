TRRTRL_UNT_DS_CJ :=
    cross_join(TRRTRL_UNT_DS_MSR, DT_RFRNC_DS);

TRRTRL_UNT_DS_FLTR :=
    TRRTRL_UNT_DS_CJ[filter DT_RFRNC >= VLD_FRM and DT_RFRNC <= VLD_T];
        
TRRTRL_UNT_DS := 
