[flake8]
application-import-names = vtlengine
docstring-convention = google
extend-ignore = E203,D411,W503,S410,S320
import-order-style = google
max-complexity = 20
max-line-length = 100
select = B,B9,BLK,C,C4,D,DAR,E,F,I,PT,S,SIM,W
per-file-ignores = tests/*:S101,D100,D103,D104,E501,W605 src/vtlengine/Utils/__init__.py:F405 src/vtlengine/*:E712
strictness=short
exclude = */Grammar/*